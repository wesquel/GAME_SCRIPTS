   "source": [
    "def force(move):\n",
    "    link = 'https://pokemondb.net/move/'+str(move.replace(\" \",\"-\").lower())\n",
    "    movee = Request(link, headers={'User-Agent': 'Mozilla/5.0'})\n",
    "    page2 = urlopen(movee).read()\n",
    "    soup2 = BeautifulSoup(page2, 'html5lib')\n",
    "    all_table2 = soup2.find_all('table')\n",
    "    power = all_table2[0]\n",
    "    coutnk = 0\n",
    "    for row in power.findAll(\"tr\"): #para tudo que estiver em <tr>\n",
    "        cells = row.findAll('td') #variável para encontrar <td>\n",
    "        if coutnk == 2:\n",
    "            return str(cells).replace('[<td>','').replace('</td>]',\"\")\n",
    "        coutnk = coutnk + 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 174,
   "metadata": {},
   "outputs": [],
   "source": [
    "title = []\n",
    "move_title = []\n",
    "tipo = []\n",
    "for row in g1.findAll(\"tr\"): #para tudo que estiver em <tr>\n",
    "    cells = row.findAll('td') #variável para encontrar <td>\n",
    "    if len(cells) > 1:\n",
    "        title.append(cells[0].find(True)['title'])\n",
    "        move_title.append(cells[1].find(True)['title'].replace(\" (move)\",''))\n",
    "        tipo.append(cells[2].find(True)['title'].replace(\" (type)\",''))"
   ]
